USR_NAME="WestleyR"
PKG_NAME="pac"
NAME="pac"
PKG_VERSION="1.0.0"

SHA256SUM="1b8e6857c2627396c4dc5a965c909a656bd2b1b9ef1a841ddcad1dc48abbec79"

