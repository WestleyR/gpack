USR_NAME="WestleyR"
PKG_NAME="pac"
NAME="pac"
PKG_VERSION="1.0.0"

TARBALL="https://github.com/${USR_NAME}/${PKG_NAME}/archive/v${PKG_VERSION}.tar.gz"
UNTAR_DIR="${NAME}-${PKG_VERSION}"

COMMANDS="pac"
LIBRARIES=""

# Load the prefix
. load_gpack ${PKG_NAME}

INSTALL_CMD="make install PREFIX=${gpack_prefix}"

